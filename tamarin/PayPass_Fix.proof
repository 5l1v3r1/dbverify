theory PayPass_Fix begin

// Function signature and definition of the equational theory E

builtins: multiset
functions: MAC/4, adec/2, aenc/2, fst/1, pair/2, pk/1, sdec/2, senc/2,
           sign/2, snd/1, true/0, verify/3
equations:
    adec(aenc(x.1, pk(x.2)), x.2) = x.1,
    fst(<x.1, x.2>) = x.1,
    sdec(senc(x.1, x.2), x.2) = x.1,
    snd(<x.1, x.2>) = x.2,
    verify(sign(x.1, x.2), x.1, pk(x.2)) = true



rule (modulo E) InitCounter:
   [ ] --[ Once( <$R, $C, 'ATC'> ) ]-> [ CounterInReader( $R, $C, '0' ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) Card1:
   [ Fr( ~nC ), Fr( ~ti ), !Ltk( $C, skC ) ]
  --[ Start( ~nC ) ]->
   [
   CardState1( $C, ~nC, sign(~nC, skC), ~ti ),
   MayLeak( $C, <~nC, sign(~nC, skC), ~ti> )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) Reader1:
   [ Fr( ~UN ) ]
  --[ Start( ~UN ), Send( $R, <'ExchRelayResistData', ~UN> ) ]->
   [ ReaderState1( $R, ~UN ), Out( <'ExchRelayResistData', ~UN> ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) Card2:
   [ CardState1( C, nC, nonceSigned, ti ), In( <'ExchRelayResistData', UN> )
   ]
  -->
   [ CardState2( C, nC, ti, UN ), Send( C, <nC, ti, nonceSigned, UN> ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) Reader2:
   [
   ReaderState1( R, UN ), Fr( ~Amount ),
   Recv( R, <nC, ti, nonceSigned, UN> )
   ]
  -->
   [
   ReaderState2( R, UN, nC, nonceSigned, ti, ~Amount ),
   Out( <'GenerateAC', UN, ~Amount, 'Currency'> )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) Card3:
   [
   CardState2( C, nC, ti, UN ), !Shk( C, $R, KM ), !Ltk( C, skC ),
   In( <'GenerateAC', UN, Amount, 'Currency'> ), In( <'AdvKnows_ATC', ATC> )
   ]
  --[ End( nC ) ]->
   [
   Send( C,
         <sign(<nC, UN, MAC(senc(ATC, KM), Amount, ATC, UN), ti>, skC), 
          MAC(senc(ATC, KM), Amount, ATC, UN), ATC>
   )
   ]

  /* has exactly the trivial AC variant */

