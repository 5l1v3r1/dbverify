               (Action( P ) @ #t2)) ∧
              (Send( V, nv ) @ #t3)) ∧
             (Action( P ) @ #t4)) ∧
            (Recv( V, <nv, xor(np, P)> ) @ #t5)) ∧
           (Action( P ) @ #t6)) ∧
          (DBSec( V, P, nv, <nv, xor(np, P)> ) @ #t7)) ∧
         (#t1 < #t2)) ∧
        (#t2 < #t3)) ∧
       (#t3 < #t4)) ∧
      (#t4 < #t5)) ∧
     (#t5 < #t6)) ∧
    (#t6 < #t7)"
/*
guarded formula characterizing all satisfying traces:
"∃ V P nv np #t1 #t2 #t3 #t4 #t5 #t6 #t7.
  (Start( nv ) @ #t1) ∧
  (Start( np ) @ #t2) ∧
  (Action( P ) @ #t2) ∧
  (Send( V, nv ) @ #t3) ∧
  (Action( P ) @ #t4) ∧
  (Recv( V, <nv, xor(np, P)> ) @ #t5) ∧
  (Action( P ) @ #t6) ∧
  (DBSec( V, P, nv, <nv, xor(np, P)> ) @ #t7)
 ∧
  (#t1 < #t2) ∧
  (#t2 < #t3) ∧
  (#t3 < #t4) ∧
  (#t4 < #t5) ∧
  (#t5 < #t6) ∧
  (#t6 < #t7)"
*/
simplify
solve( VerifierState1( $B, nv ) ▶₀ #t3 )
  case Verifier1
  solve( Send( $A, <~nv, xor(np, P)> ) ▶₀ #t5 )
    case Adv_case_1
    solve( VerifierState3( $B, ~nv, xor(np, P) ) ▶₀ #t7 )
      case Verifier3_case_1
      solve( !Shk( $B, $A.1, k ) ▶₂ #t7 )
        case SymKey
        solve( Start( ~nv ) @ #t1 )
          case Verifier1
          solve( Start( ~np ) @ #t2 )
            case Prover1
            solve( Action( $A ) @ #t2 )
              case Prover1
              solve( Action( $A ) @ #t4 )
                case Adv
