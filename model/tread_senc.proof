  case Verifier2
  solve( Recv( $V, f(~c, <a, XOR(b, ~m)>) ) ▶₁ #l )
    case DBNet_case_1
    solve( !Shk( $V, $P, shk ) ▶₂ #l )
      case ShkGen
      solve( !KeyComp( $P ) @ #j )
        case LtkReveal
        solve( !Ltk( $P, k ) ▶₀ #j )
          case LtkGen
          solve( !KU( senc(<<~a, ~b, $P>, sign(<~a, ~b, $P>, ~k)>, ~k.1)
                 ) @ #vk.1 )
