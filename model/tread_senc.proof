                  case ShkRevealX
                  solve( !KU( sign(<~a, ~b, $P>, ~k) ) @ #vk.12 )
                    case c_sign
                    solve( !KU( ~c ) @ #vk.10 )
                      case Verifier2
                      solve( !KU( ~m ) @ #vk.11 )
                        case Verifier1
