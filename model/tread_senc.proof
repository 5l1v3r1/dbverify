    solve( !Shk( $V, $P, shk ) ▶₂ #l )
      case ShkGen
      solve( !KU( senc(<<~a, ~b, $P>, sign(<~a, ~b, $P>, ~k.1)>, ~k)
             ) @ #vk.1 )
        case c_senc
        solve( !KU( ~k ) @ #vk.7 )
