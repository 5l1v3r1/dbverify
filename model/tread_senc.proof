   [
   Send( $P, <senc(<<~a, ~b, $P>, sign(<~a, ~b, $P>, ltkP)>, shk), $P> ),
   ProverState1( $V, $P, ~a, ~b )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) Verifier1:
